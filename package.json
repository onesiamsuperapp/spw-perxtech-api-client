{
  "name": "spw-perxtech-api-client",
  "version": "1.0.2",
  "description": "PerxTech API client written in TypeScript",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc -p tsconfig.cjs.json",
    "clean": "rm -rf node_modules/ && rm -rf libs/",
    "lint": "eslint --max-warnings=0 src",
    "prepare": "npm run build && husky install",
    "test:unit": "jest --config=jest.config.ts --coverage",
    "test:integration": "jest --config=jest-integration.config.ts --runInBand --detectOpenHandles --bail"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onesiamsuperapp/spw-perxtech-api-client.git"
  },
  "keywords": [
    "perxtech",
    "client",
    "typescript"
  ],
  "author": "XPO Dev Team",
  "license": "ISC",
  "types": "./src/index.ts",
  "dependencies": {
    "cerialize": "0.1.18",
    "spw-instrumentation": "1.0.1"
  },
  "peerDependencies": {
    "axios": ">=0.21.1",
    "lodash": ">=4.17.21",
    "@mikro-orm/core": ">=4.5.3",
    "@mikro-orm/mysql-base": ">=4.5.3",
    "spw-kobp": ">=0.0.42"
  },
  "devDependencies": {
    "@types/jest": "26.0.20",
    "@types/lodash": "4.14.168",
    "@types/node": "14.18.54",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "axios": "0.21.1",
    "dotenv": "10.0.0",
    "eslint": "7.32.0",
    "husky": "7.0.4",
    "jest": "26.6.3",
    "lodash": "4.17.21",
    "ts-jest": "26.5.3",
    "ts-node": "9.1.1",
    "typescript": "4.4.4",
    "@mikro-orm/core": "4.5.3",
    "@mikro-orm/mysql-base": "4.5.3",
    "nock": "^13.3.2"
  },
  "optionalDependencies": {
    "@commitlint/cli": "17.6.7",
    "@commitlint/config-conventional": "17.6.7",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "9.0.2",
    "semantic-release": "19.0.5"
  }
}
